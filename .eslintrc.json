{
    "extends": "standard",
    "rules": {
        "space-before-function-paren": ["warn", {"anonymous": "always", "named": "never", "asyncArrow": "always"}],
        "no-trailing-spaces": "off",
        "no-multiple-empty-lines": "off",
        "no-unused-vars": "off",
        "padded-blocks": "off",
        "space-infix-ops": "warn"
    }
}
